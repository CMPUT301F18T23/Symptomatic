        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/back_left_ankle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/back_left_calve"
        app:srcCompat="@drawable/back_left_foot" />

    <ImageButton
