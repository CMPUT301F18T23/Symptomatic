        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintBottom_toTopOf="@+id/back_left_thigh"
        app:layout_constraintEnd_toStartOf="@+id/right_buttox"
        app:layout_constraintStart_toEndOf="@+id/back_left_forearm"
        app:layout_constraintTop_toBottomOf="@+id/lower_back"
        app:srcCompat="@drawable/back_left_buttox" />

    <ImageButton
        android:id="@+id/right_buttox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:adjustViewBounds="false"
