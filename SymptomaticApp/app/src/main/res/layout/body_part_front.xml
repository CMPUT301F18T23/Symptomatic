<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/background"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/face"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="131dp"
        android:layout_marginLeft="131dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="141dp"
        android:layout_marginRight="141dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintBottom_toTopOf="@+id/upper_chest"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="spread_inside"
        app:srcCompat="@drawable/face" />

    <ImageButton
        android:id="@+id/upper_chest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintEnd_toStartOf="@+id/front_left_shoulder"
        app:layout_constraintStart_toEndOf="@+id/front_right_shoulder"
        app:layout_constraintTop_toBottomOf="@+id/face"
        app:srcCompat="@drawable/upper_chest" />

    <ImageButton
        android:id="@+id/abdomen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintBottom_toTopOf="@+id/groin"
        app:layout_constraintEnd_toStartOf="@+id/front_left_forearm"
        app:layout_constraintStart_toEndOf="@+id/right_bicep"
        app:layout_constraintTop_toBottomOf="@+id/upper_chest"
        app:srcCompat="@drawable/abdomen" />

    <ImageButton
        android:id="@+id/groin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintBottom_toTopOf="@+id/front_right_thigh"
        app:layout_constraintEnd_toStartOf="@+id/front_left_forearm"
        app:layout_constraintStart_toEndOf="@+id/front_right_forearm"
        app:layout_constraintTop_toBottomOf="@+id/abdomen"
        app:srcCompat="@drawable/groin" />

    <ImageButton
        android:id="@+id/front_right_thigh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginLeft="29dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintEnd_toStartOf="@+id/front_left_thigh"
        app:layout_constraintStart_toEndOf="@+id/front_right_hand"
        app:layout_constraintTop_toBottomOf="@+id/groin"
        app:srcCompat="@drawable/front_right_thigh" />

    <ImageButton
        android:id="@+id/front_left_thigh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="17dp"
        android:layout_marginRight="17dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintEnd_toStartOf="@+id/front_left_hand"
        app:layout_constraintStart_toEndOf="@+id/front_right_thigh"
        app:layout_constraintTop_toBottomOf="@+id/groin"
        app:srcCompat="@drawable/back_right_thigh" />

    <ImageButton
        android:id="@+id/left_shin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="152dp"
        android:layout_marginRight="152dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/right_shin"
        app:layout_constraintTop_toBottomOf="@+id/front_left_knee"
        app:srcCompat="@drawable/left_shin" />

    <ImageButton
        android:id="@+id/front_left_knee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="155dp"
        android:layout_marginRight="155dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/front_right_knee"
        app:layout_constraintTop_toBottomOf="@+id/front_left_thigh"
        app:srcCompat="@drawable/front_left_knee" />

    <ImageButton
        android:id="@+id/front_left_foot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="148dp"
        android:layout_marginRight="148dp"
        android:layout_marginBottom="17dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/front_right_foot"
        app:layout_constraintTop_toBottomOf="@+id/left_shin"
        app:srcCompat="@drawable/front_left_foot" />

    <ImageButton
        android:id="@+id/front_right_foot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="132dp"
        android:layout_marginLeft="132dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="9dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/front_left_foot"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/right_shin"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/front_right_foot" />

    <ImageButton
        android:id="@+id/right_shin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="135dp"
        android:layout_marginLeft="135dp"
        android:layout_marginEnd="7dp"
        android:layout_marginRight="7dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintEnd_toStartOf="@+id/left_shin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/front_right_knee"
        app:srcCompat="@drawable/right_shin" />

    <ImageButton
        android:id="@+id/front_right_knee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginLeft="140dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintEnd_toStartOf="@+id/front_left_knee"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/front_right_thigh"
        app:srcCompat="@drawable/front_right_knee" />

    <ImageButton
        android:id="@+id/front_left_shoulder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="102dp"
        android:layout_marginEnd="101dp"
        android:layout_marginRight="101dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/upper_chest"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/front_left_shoulder" />

    <ImageButton
        android:id="@+id/left_bicep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginEnd="93dp"
        android:layout_marginRight="93dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintBottom_toTopOf="@+id/front_left_forearm"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/abdomen"
        app:layout_constraintTop_toBottomOf="@+id/front_left_shoulder"
        app:srcCompat="@drawable/left_bicep" />

    <ImageButton
        android:id="@+id/front_left_forearm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="84dp"
        android:layout_marginRight="84dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintBottom_toTopOf="@+id/front_left_hand"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/left_bicep"
        app:srcCompat="@drawable/front_left_forearm" />

    <ImageButton
        android:id="@+id/front_left_hand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:layout_marginBottom="238dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/front_left_forearm"
        app:srcCompat="@drawable/front_left_hand" />

    <ImageButton
        android:id="@+id/front_right_shoulder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="67dp"
        android:layout_marginLeft="67dp"
        android:layout_marginTop="57dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintEnd_toStartOf="@+id/upper_chest"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/saveclose"
        app:srcCompat="@drawable/front_right_shoulder" />

    <ImageButton
        android:id="@+id/right_bicep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="77dp"
        android:layout_marginLeft="77dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintBottom_toTopOf="@+id/front_right_forearm"
        app:layout_constraintEnd_toStartOf="@+id/abdomen"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/front_right_shoulder"
        app:srcCompat="@drawable/right_bicep" />

    <ImageButton
        android:id="@+id/front_right_forearm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="50dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintBottom_toTopOf="@+id/front_right_hand"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/right_bicep"
        app:srcCompat="@drawable/front_right_forearm" />

    <ImageButton
        android:id="@+id/front_right_hand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:layout_marginLeft="43dp"
        android:layout_marginBottom="243dp"
        android:adjustViewBounds="false"
        android:background="@null"
        android:cropToPadding="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/front_right_forearm"
        app:srcCompat="@drawable/front_right_hand" />

    <Button
        android:id="@+id/saveclose"
        android:layout_width="wrap_content"
        android:layout_height="44dp"
        android:text="Save"
        android:background="@color/logoColor"
        android:textColor="@color/background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/linkToBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/logoColor"
        android:textColor="@color/background"
        android:text="Back View"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>